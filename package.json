{
  "name": "shellify",
  "version": "0.0.5",
  "description": "Tools for Node.js command line interfaces",
  "dependencies": {
    "cedar": "^0.1.1"
  },
  "devDependencies": {
    "zeriousify": "^0.1.10",
    "exam": "^0.1.5",
    "istanbul": "0.2.11",
    "coveralls": "2.10.0"
  },
  "keywords": [
    "shellify"
  ],
  "main": "shellify.js",
  "bin": {
    "shellify": "shellify.js"
  },
  "engines": {
    "node": ">=0.2.6"
  },
  "scripts": {
    "test": "./node_modules/exam/exam.js",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/exam/exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": "https://github.com/lighterio/shellify.git",
  "bugs": "https://github.com/lighterio/shellify/issues",
  "homepage": "http://github.com/lighterio/shellify",
  "author": "Sam Eubank <sameubank@gmail.com>",
  "contributors": [
    "Sam Eubank <sameubank@gmail.com>"
  ],
  "license": "MIT"
}
